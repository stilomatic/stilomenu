package it.quidcom.behavior{
	
	import flash.events.*;
	import it.quidcom.menu.AbstractMenu;
	
	/** 
	* The AbstractBehavior class, is the abstract class for any behavior of the menu package. 
	* Implements the interface <code>IBehavior</code>.
	*  
	* @author Antonio Stilo
	*/
	public class AbstractBehavior implements IBehavior{
		
		protected var menu:AbstractMenu;
		protected var values:Object;

		public function AbstractBehavior (){
			super();
			}
			
		public function get target():AbstractMenu{
			return menu;
			}
			
		 public function set target(_menu:AbstractMenu):void{
				menu = _menu; 
			}		
		}
	}